PROG fact
FUNC factorial (I )I
{
  TEMP 0:I;
  TEMP 1:I;
  TEMP 2:I;
  TEMP 3:Z;
  TEMP 4:Z;
  TEMP 5:I;
  TEMP 6:I;
  TEMP 7:I;
  TEMP 8:F;
  TEMP 9:I;
  TEMP 10:I;
  TEMP 11:I;
  T2 := 1;
  T3 := T0 I== T2;
  T4 := Z! T3;
  IF T4 GOTO L0;
  T5 := 1;
  T1 := T5;
  GOTO L1;
L0:;
  T6 := 1;
  T7 := T0 I- T6;
  T9 := CALL factorial(T7 );
  T10 := T0 I* T9;
  T1 := T10;
L1:;
  RETURN T1;
  T11 := 0;
  RETURN T11;
}
FUNC main ()V
{
  TEMP 0:U;
  TEMP 1:I;
  TEMP 2:F;
  TEMP 3:I;
  T0 := "The factorial of 8 is ";
  PRINTU T0;
  T1 := 8;
  T3 := CALL factorial(T1 );
  PRINTLNI T3;
  RETURN;
}
